{
  "name": "insighthunter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "deploy": "wrangler deploy",
    "migrate": "wrangler d1 migrations apply insighthunter-db",
    "seed": "wrangler d1 execute insighthunter-db --file=seeds/001_seed.sql"
  },
  "dependencies": {
    "next": "14.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.80.0",
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/react": "^18.2.21",
    "@types/node": "^20.11.30",
    "vitest": "^2.1.4",
    "@vitest/coverage-v8": "^2.1.4",
    "miniflare": "^3.0.2",
    "playwright": "^1.49.0",
    "@playwright/test": "^1.49.0",
    "@testing-library/react": "^14.2.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@cloudflare/workers-types": "^4.20241218.0"
  }
}
